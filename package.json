{
  "name": "duilib-ui-preview",
  "displayName": "DuiLib UI Preview",
  "publisher": "museray",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luleiray/vscode-duilibxml-preview"
  },
  "description": "Preview DuiLib's XML",
  "icon": "assets/logo.png",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "duilib_preview_xml",
        "title": "DuiLib Preview"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "duilib_preview_xml",
          "when": "resourceExtname == '.xml'"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "pack": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.60.0",
    "@types/xmldoc": "^1.1.9",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "^2.20.0",
    "eslint": "^8.56.0",
    "lodash": "latest",
    "shelljs": "latest",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "xmldoc": "latest"
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
